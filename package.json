{
  "name": "polania-zdrowie",
  "version": "1.0.0",
  "description": "polania-zdrowie",
  "main": "index.js",
  "scripts": {
    "analyzeReact": "npm run analyze:webpack && npm run analyze:resultsReact",
    "analyze:webpack": "webpack -p --json --progress --profile > webpack-build-log.json",
    "analyze:resultsReact": "webpack-bundle-analyzer webpack-build-log.json distReact",
    "build": "webpack --config webpack.prod.config.js",
    "prepare": "npm run build",
    "start:dev": "webpack-dev-server --config webpack.dev.config.js --content-base dev/",
    "test": "npm run test-eslint & npm run test-jest",
    "test-eslint": "eslint --ignore-path .gitignore **/*.js **/*.jsx",
    "test-jest": "SET NODE_ENV=test & jest --coverage",
    "test-jest:watch": "jest --watch"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>__tests__/setup/setupTest.js",
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rook91/min-react-app.git"
  },
  "keywords": [
    "react",
    "app"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rook91/min-react-app/issues"
  },
  "homepage": "https://github.com/rook91/min-react-app#readme",
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router5": "^6.5.3",
    "router5": "^6.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel7-jest": "^1.0.1",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "regenerator-runtime": "^0.13.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
